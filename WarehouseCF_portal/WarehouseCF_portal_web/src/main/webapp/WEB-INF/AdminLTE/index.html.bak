<script>
    //动态获取菜单
    $(function () {
        var firstLvId=getFirstLvMenu();//一级菜单
        var secondLvId=getSecondLvMenu();//二级菜单
        var thirdLvId=getThirdLvMenu();//三级菜单
        console.log(firstLvId);
        $.ajax({
            type: 'get',
            url:'/mainPage/getTree',
            dataType:'json',
            success:function(data){
                var menu=null;
                var html=null;
                var childLen=null;
                var child=null;
                var json=data;
                // console.log(json);
                for(var i in json){
                    menu=json[i];
                    //如果父菜单是该菜单，就展开
                    if(menu.menuid==firstLvId){
                        html=$('<li menu-id="'+i+'" class="active treeview "><li>');
                    }else{
                        html=$('<li menu-id="'+i+'" class="treeview "><li>');
                    }
                    $(".sidebar .sidebar-menu").append(html);
                    html=$('<a class="first-menu" href="../'+menu.menuurl+'" onclick="saveFirstLvMenu('+menu.menuid+')"><i class="fa fa-dashboard"></i> <span>'+menu.menuname+'</span><span class="pull-right-container"><i class="fa fa-angle-left pull-right"></i></span></a><ul menuUL-id="'+i+'" class="treeview-menu"></ul>');
                    $('[menu-id="'+i+'"]').append(html);
                    //继续遍历二级菜单
                    childLen=menu.child.length;
                    for(var j in menu.child){
                      child=menu.child[j];
                        //如果子菜单是该菜单，则设为active选中
                        if(child.id==secondLvId){
                            html=$('<li class="active"><a class="second-menu" href="../'+child.menuurl+'" target="menuFrame" onclick="saveSecondLvMenu('+child.menuid+')"><i class="fa fa-circle-o"></i>'+child.menuname+'</a></li>');
                        }else{
                            html=$('<li><a class="second-menu" href="../'+child.menuurl+'" target="menuFrame" onclick="saveSecondLvMenu('+child.menuid+')"><i class="fa fa-circle-o"></i>'+child.menuname+'</a></li>');
                        }
                        $('[menuUL-id="'+i+'"]').append(html);
                    }
                }
            }

        });
    });
    function saveFirstLvMenu(menuid) {
        var id = JSON.stringify(menuid);
        window.sessionStorage.setItem("firstMenuId", id);
    }

    function saveSecondLvMenu(menuid) {
        var id = JSON.stringify(menuid);
        window.sessionStorage.setItem("secondMenuId", id);
    }

    function saveThirdLvMenu(menuid) {
        var id = JSON.stringify(menuid);
        window.sessionStorage.setItem("thirdMenuId", id);
    }

    function getFirstLvMenu() {
      return window.sessionStorage.getItem("firstMenuId");
    }
    function getSecondLvMenu() {
      return window.sessionStorage.getItem("secondMenuId");
    }
    function getThirdLvMenu() {
      return window.sessionStorage.getItem("thirdMenuId");
    }

